<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--伐木-->
    <RaidStrategyDef Name="RaidStrategyDefSrLogging">
        <defName>SrLogging</defName>
        <workerClass>SR.ModRimworld.RaidExtension.RaidStrategyWorkerLogging</workerClass>
        <selectionWeightPerPointsCurve>
            <points>
                <li>(1,0)</li>
            </points>
        </selectionWeightPerPointsCurve>
        <pointsFactorCurve>
            <points>
                <li>0, 1</li>
            </points>
        </pointsFactorCurve>
        <arriveModes>
            <li>EdgeWalkIn</li>
        </arriveModes>
        <letterLabelEnemy>Logging</letterLabelEnemy>
        <arrivalTextEnemy>They plan to log wood nearby.</arrivalTextEnemy>
    </RaidStrategyDef>
    <RaidStrategyDef ParentName="RaidStrategyDefSrLogging">
        <defName>SrLoggingSurprise</defName>
        <workerClass>SR.ModRimworld.RaidExtension.RaidStrategyWorkerLoggingSurprise</workerClass>
    </RaidStrategyDef>
